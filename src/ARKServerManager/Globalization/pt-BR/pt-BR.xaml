    <sys:String x:Key="ServerSettings_CrossplayTooltip">Se ativado, ativará o Cross Play entre Steam e Epic Games.</sys:String>
    <sys:String x:Key="ServerSettings_EpicOnlyLabel">Somente jogadores da Epic Games</sys:String>
